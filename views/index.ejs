<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>OenCV4NodeJS</title>
    <script src="socket.io/socket.io.js"></script>
</head>

<body>
    <div style="text-align: center;">
        <h1>camgtlhp555</h1>
        <img id="camgtlhp555" width="1280"/>
    </div>

</body>

<script>
    let socket = io.connect("/");
    socket.on('camgtlhp555_image', (image) => {
        //console.log(image);
        let imageElm = document.getElementById('camgtlhp555');
        imageElm.src = `data:image/jpeg;base64,${image}`;
    })
</script>

</html>